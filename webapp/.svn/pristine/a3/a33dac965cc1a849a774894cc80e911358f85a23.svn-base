define(["app"],function(app){
    app.orderStatusToString = function(str){
        str = parseInt(str);
        if( str == 0 ){
            return '未转账';
        }
        if( str == 1 ){
            return '已转账';
        }
        if( str == 2 ){
            return '待收货';
        }
        if( str == 3 ){
            return '已完成';
        }
        if( str == 4){
            return '已取消';
        }
    }
})