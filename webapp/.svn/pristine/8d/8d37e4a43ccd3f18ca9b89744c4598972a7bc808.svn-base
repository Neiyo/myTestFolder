<div class="am-cf am-padding am-padding-bottom-0">
    <div class="am-fl am-cf">
        <strong class="am-text-primary am-text-lg">商品</strong> /
        <small>goods</small>
    </div>
</div>

<hr>
<div class="am-g">
    <div class="am-u-lg-4 am-u-sm-12 am-u-md-6">
        <div class="am-input-group am-input-group-primary">
            <input type="text" class="am-form-field">
            <span class="am-input-group-btn">
				<button class="am-btn am-btn-primary" type="button"><span class="am-icon-search"></span></button>
			  </span>
        </div>
    </div>
    <div class="shoppingCar cartfd am-u-lg-6 am-u-sm-12 am-u-md-6">
        <a href="http://192.168.2.125/html/amazeUI/goods_list.html" class="am-fr cartit">
            <span><s></s>去购物车结算</span>
            <strong class="cartNum">7</strong>
        </a>
    </div>
</div>

<ul class="am-avg-sm-2 am-avg-md-4 am-avg-lg-6 am-margin gallery-list">

    <li ng-repeat="item in pageData.list">
        <div href="javascript:void(0)" onclick="selectNum()">
            <img class="am-img-thumbnail am-img-bdrs" ng-src="{{proPics(item.goodsInfoImgId)}}" alt="">
            <p class="goods-name">{{proPics(item.goodsInfoName)}}</p>
            <div class="goodsFoot">
                <span class="goods-price am-fl">{{item.goodsInfoPreferPrice}}</span>
                <button type="button" class="am-btn am-btn-default am-radius am-btn-xs am-fr">加入购物车</button>
            </div>
        </div>
    </li>

</ul>

<div class="am-modal am-modal-prompt" tabindex="-1" id="my-prompt">
    <div class="am-modal-dialog">
        <div class="am-modal-hd">请输入你要购买的数量：</div>
        <div class="am-modal-bd">
            <form class="numForm">
                <input type="text" class="am-modal-prompt-input required digits" value="1">
            </form>
        </div>
        <div class="am-modal-footer">
            <span class="am-modal-btn" data-am-modal-cancel="">取消</span>
            <span class="am-modal-btn" data-am-modal-confirm="">加入购物车</span>
        </div>
    </div>
</div>


<page-view></page-view>

